<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginController">
   <children>
      <Button fx:id="submitButton" layoutX="84.0" layoutY="243.0" mnemonicParsing="false" onAction="#login" prefHeight="25.0" prefWidth="132.0" text="Submit" />
      <Label layoutX="56.0" layoutY="70.0" text="IP:" />
      <Label layoutX="56.0" layoutY="142.0" text="Port:" />
      <TextField fx:id="ipAddress" layoutX="84.0" layoutY="66.0" promptText="IP" />
      <TextField fx:id="port" layoutX="84.0" layoutY="138.0" promptText="Port" />
      <Label alignment="CENTER" layoutX="112.0" layoutY="29.0" prefHeight="17.0" prefWidth="82.0" text="Login" textFill="#eb0505">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="ipCompletion" alignment="CENTER_RIGHT" layoutX="82.0" layoutY="99.0" prefHeight="17.0" prefWidth="149.0" />
      <Label fx:id="portCompletion" alignment="CENTER_RIGHT" layoutX="84.0" layoutY="173.0" prefHeight="17.0" prefWidth="149.0" />
      <Label fx:id="labelStatus" layoutX="136.0" layoutY="213.0" />
   </children>
</AnchorPane>
